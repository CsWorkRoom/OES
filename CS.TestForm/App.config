<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <!--系统表主键是否自增长-->
    <add key="KeyFieldIsAutoIncrement" value="false"/>

    <!--日志等级，填写数字：(高) OFF【0】 > FATAL【1】 > ERROR【2】 > WARN【3】 > INFO【4】 > DEBUG【5】 (低) -->
    <add key="LogWriteLevel" value="5"/>
    <!--数据库类型-->
    <add key="DataBaseType" value="Oracle"/>
    <!--<add key="DataBaseType" value="DB2" />-->
    <!--数据库连接字符串-->
    <!--<add key="ConnString" value="User ID=C##DY_FAS2;Password=C##DY_FAS2;Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=139.196.212.68)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=ORCL)));Persist Security Info=True;"/>-->
    <!--<add key="ConnString" value="Server=114.116.50.232:50000;UID=C##DY_FAS2;PWD=easyman123@@@;DataBase=DMDY;"/>-->
    <!--<add key="ConnString" value="User ID=SMS;Password=SMS;Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=139.196.212.68)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=ORCL)));Persist Security Info=True;"/>-->
    <!--<add key="ConnString" value="Server=114.116.50.232:50000;UID=C##CSO;PWD=Easyman@@@;DataBase=DMDY;"/>-->
    <!--<add key="ConnString" value="User ID=AJTM;Password=AJTM;Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=127.0.0.1)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=ORCL)));Persist Security Info=True;"/>-->
    <add key="ConnString" value="User ID=OES;Password=OES;Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=219.151.157.101)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=ORCL)));Persist Security Info=True;"/>
    <!--附件存放路径（框架中使用）-->
    <add key="AttachmentPath" value="D:\www\QSS\Web\Attachment"/>

    <!--标签管理附件的路径-->
    <add key="LabelPath" value="D:\www\QSS\Web\LabelPath"/>
    <!--指标管理附件的路径-->
    <add key="TargetPath" value="D:\www\QSS\Web\TargetPath"/>
    <!--指标过滤管理附件的路径-->
    <add key="FilterPath" value="D:\www\QSS\Web\FilterPath"/>
    <!--计件政策管理附件的路径-->
    <add key="JjPolicyPath" value="D:\www\QSS\Web\JjPolicyPath"/>
    <!--抢盘政策管理附件的路径-->
    <add key="QpPolicyPath" value="D:\www\QSS\Web\QpPolicyPath"/>
    <!--抢盘模板管理附件的路径-->
    <add key="QpTemplatePath" value="D:\www\QSS\Web\QpTemplatePath"/>
    <!--获取分享政策管理附件的路径-->
    <add key="FxPolicyPath" value="D:\www\QSS\Web\FxPolicyPath"/>
    <!--薪酬模版管理附件的路径-->
    <add key="TemplatePath" value="D:\www\QSS\Web\TemplatePath"/>

    <!--是否与4A对接：true=对接；false=不对接 -->
    <add key="IsConnectTo4A" value="false"/>
    <!--审计接口地址：目前21010端口已坏，请使用19094或21030端口做策略-->
    <!--<add key="OpLogAddress" value="http://10.109.209.90:21010/gather/services/AioxPort" />-->
    <add key="OpLogAddress" value="http://10.109.209.90:19094/gather/services/AioxPort" />
    <!--<add key="OpLogAddress" value="http://10.109.209.90:21030/gather/services/AioxPort" />-->
