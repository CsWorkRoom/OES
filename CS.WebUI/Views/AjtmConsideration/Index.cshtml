@model CS.WebUI.Controllers.Model.AsPersonnel
@{
    Layout = "~/Views/Shared/_Content.cshtml";
    //
    var Typelist = ViewBag.LeaderType as List<CS.BLL.Model.AJTM_LEADER_TYPE.Entity>;
}
<form id="form" class="layui-form" action="" method="post">
    <input hidden="hidden" id="IDS" name="IDS" value="@ViewBag.IDS" />
    <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
        <legend>审议表</legend>
    </fieldset>
    <div class="layui-form-item" id="LeaderStart">
        <div class="layui-inline">
            <label class="layui-form-label">审议表名</label>
            <div class="layui-input-inline">
                <input type="text" name="NAME" id="NAME" value="@ViewBag.NAME" autocomplete="off" lay-verify="required |name" class="layui-input">
            </div>
        </div>
    </div>
    <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
        <legend>审议表信息(非必填)</legend>
    </fieldset>
    <div class="layui-form-item" id="LeaderStart">
        <div class="layui-inline">
            <label class="layui-form-label">xls时间</label>
            <div class="layui-input-inline">
                <input type="text" name="xTime" id="xTime" value="" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">数据审核</label>
            <div class="layui-input-inline">
                <input type="text" name="xApprove" id="xApprove" value="" autocomplete="off" class="layui-input">
            </div>
        </div>
    </div>
    <div class="layui-form-item" id="LeaderStart">
        <div class="layui-inline">
            <label class="layui-form-label">经办人</label>
            <div class="layui-input-inline">
                <input type="text" name="xDeal" id="xDeal" value="" autocomplete="off" class="layui-input">
            </div>
        </div>
    </div>
    <div class="form-bottom-submit" style="text-align:right;">
        <input type="hidden" id="msg" value="@ViewBag.Message" />
        <div class="layui-inline">
            <button type="button" class="layui-btn" onclick="save();">确定</button>
            <button type="reset" class="layui-btn layui-btn-primary" onclick="CloseForm();">关闭</button>
        </div>
    </div>
</form>
<script src="~/Views/AjtmConsideration/JS/Index.js?v=20210614"></script>