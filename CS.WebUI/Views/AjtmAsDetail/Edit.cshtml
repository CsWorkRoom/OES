@model CS.WebUI.Controllers.Model.AsDetail
@{
    Layout = "~/Views/Shared/_Content.cshtml";
}
<form id="form" class="layui-form" action="" method="post">
    <script src="~/Content/FW/LayuiZtree.js?v=20210609"></script>
    <input hidden="hidden" id="AsType" name="AsType" value="@ViewBag.AsType" />
    <input hidden="hidden" id="Unit" name="Unit" value="@ViewBag.Unit" />
    <input hidden="hidden" id="ID" name="ID" value="@Model.ID" />
    <input hidden="hidden" id="AsPurpose" name="AsPurpose" value="@ViewBag.AsPurpose" />
    <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
        <legend>单位基本信息</legend>
    </fieldset>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">批复时间</label>
            <div class="layui-input-inline">
                <input type="text" name="APPROVAL_TIME" id="APPROVAL_TIME" value="@Model.APPROVAL_TIME" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">会议名称</label>
            <div class="layui-input-inline">
                <input type="text" name="MEETING" value="@Model.MEETING" autocomplete="off" class="layui-input">
            </div>
        </div>
    </div>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">用编单位</label>
            <div class="layui-input-inline">
                <input hidden="hidden" name="UNIT_NAME" id="UNIT_NAME" value="@Model.UNIT_NAME" />
                <input type="text" name="UNIT_ID" id="UNIT_ID" value="@Model.UNIT_ID">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">申报单位</label>
            <div class="layui-input-inline">
                <input hidden="hidden" name="UNIT_PARENT_ID" id="UNIT_PARENT_ID" value="@Model.UNIT_PARENT" />
                <input type="text" name="UNIT_PARENT" id="UNIT_PARENT" value="@Model.UNIT_PARENT" autocomplete="off" class="layui-input" readonly="readonly">
            </div>
        </div>
    </div>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">编制使用通知单</label>
            <div class="layui-input-inline">
                <input type="text" name="AS_APPLY_NO" value="@Model.AS_APPLY_NO" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">编制用途</label>
            <div class="layui-input-inline">
                <input hidden="hidden" name="AS_PURPOSE" id="AS_PURPOSE" value="@Model.UNIT_PARENT" />
                <select name="AS_PURPOSE_ID">
                    <option value=""></option>
                    @foreach (var item in ViewBag.AsPurpose)
                    {
                        if (Model.AS_PURPOSE_ID == item.Key)
                        {
                            <option value="@item.Key" selected="selected">@item.Value</option>
                        }
                        else
                        {
                            <option value="@item.Key">@item.Value</option>
                        }
                    }
                </select>
            </div>
        </div>
    </div>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">编制详细用途</label>
            <div class="layui-input-inline">
                <input type="text" name="AS_PURPOSE_REMARK" value="@Model.AS_PURPOSE_REMARK" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">编制类型</label>
            <div class="layui-input-inline">
                <input hidden="hidden" name="AS_TYPE" id="AS_TYPE" value="@Model.AS_TYPE" />
                <input type="text" name="AS_TYPE_ID" id="AS_TYPE_ID" value="@Model.AS_TYPE_ID">
            </div>
        </div>
    </div>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">用编号码</label>
            <div class="layui-input-inline">
                <input type="text" name="AS_NO" value="@Model.AS_NO" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">批准数</label>
            <div class="layui-input-inline">
                <input type="text" name="APPROVAL_NUM" value="@Model.APPROVAL_NUM" autocomplete="off" class="layui-input">
            </div>
        </div>
    </div>
</form>
<div class="form-bottom-submit" style="text-align:center;">
    <input type="hidden" id="msg" value="@ViewBag.Message" />
    <div class="layui-inline">
        <button type="button" class="layui-btn" onclick="save();">确定</button>
        <button type="reset" class="layui-btn layui-btn-primary" onclick="CloseForm();">关闭</button>
    </div>
</div>
<script src="~/Views/AjtmAsDetail/JS/Edit.js?v=20210610"></script>
