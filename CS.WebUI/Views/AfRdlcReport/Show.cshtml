@{
    Layout = "~/Views/Shared/_Content.cshtml";
}
<style>
    .layui-fluid {
        overflow: hidden !important;
    }

    .divContent {
        width: 100%;
        height: 100%;
        text-align: center;
        padding: 0px 5px 5px 0px;
    }

    .ifm {
        width: 100%;
        height: 100%;
        border: 0px;
    }

    .topCode {
        background-color: #f3f3f3;
        text-align: left;
        padding: 2px;
    }
</style>
<div class="divContent" id="divContent">
    <input id="ID" type="text" style="display:none" value="@ViewBag.ID" />
    <input type="hidden" id="QUERY_STRING" name="QUERY_STRING" value="@ViewBag.QUERY_STRING" />
     <input id="SQL_CODE" type="text" style="display:none" value="@ViewBag.SQL_CODE" />
    <div class="topCode" id="topCode">@Html.Raw(ViewBag.TOP_CODE)</div>
    <iframe class="ifm" id="ifm" src="~/Rdlc/rdlc.aspx"></iframe>
    <div class="topCode" id="botCode">@Html.Raw(ViewBag.BOTTOM_CODE)</div>
</div>

